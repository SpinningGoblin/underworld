{
  "[rust]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 4
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/target": true,
    "app/dist": true,
    "**/node_modules": true
  },
  "rust-analyzer.procMacro.attributes.enable": true,
  "cSpell.words": [
    "randomizer",
    "randomizers",
    "underworldserver"
  ]
}
